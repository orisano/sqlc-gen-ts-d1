{
  "version": "2",
  "plugins": [
    {
      "name": "ts-d1",
      "wasm": {
        "url": "file://bin/sqlc-gen-ts-d1.wasm",
        "sha256": "$WASM_SHA256"
      }
    }
  ],
  "sql": [
    {
      "schema": "schema.sql",
      "queries": "query.sql",
      "engine": "sqlite",
      "codegen": [
        {
          "out": "src/gen/sqlc",
          "plugin": "ts-d1",
          "options": "workers-types-v3=1"
        }
      ]
    }
  ]
}
