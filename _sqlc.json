{
  "version": "2",
  "plugins": [
    {
      "name": "typescript-d1",
      "wasm": {
        "url": "file://bin/sqlc-gen-typescript-d1.wasm",
        "sha256": "$WASM_SHA256"
      }
    }
  ],
  "sql": [
    {
      "schema": "schema.sql",
      "queries": "query.sql",
      "engine": "sqlite",
      "codegen": [
        {
          "out": "src/gen/sqlc",
          "plugin": "typescript-d1",
          "options": "target=wasm"
        }
      ]
    }
  ]
}
